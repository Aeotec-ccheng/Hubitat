metadata {
	definition (name: "HTTP Control Switch", namespace: "stephack", author: "Stephan Hackett") {
        capability "Switch"
        capability "Momentary"
	}
	preferences {
	}
}

def push() {
    sendEvent(name: "switch", value: "on")
    sendEvent(name: "switch", value: "off")
    parent.childOn()
}

def on() {
    
    push()
}

def off() {
    parent.childOff()
}


def installed(){}
